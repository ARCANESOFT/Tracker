{
    "name":        "arcanesoft/tracker",
    "description": "Tracker package for ARCANESOFT/Foundation.",
    "homepage":    "https://github.com/ARCANESOFT/Tracker",
    "keywords":    [
        "arcanesoft", "arcanedev", "tracker", "tracking", "logging", "visitor"
    ],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                       ">=5.6.4",
        "arcanesoft/auth":           "~1.0",
        "arcanesoft/core":           "~1.0",
        "arcanedev/laravel-tracker": "~0.7"
    },
    "require-dev": {
        "phpunit/phpcov":  "~3.0",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Tracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Tracker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
